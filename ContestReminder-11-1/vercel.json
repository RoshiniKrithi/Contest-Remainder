{
    "version": 2,
    "buildCommand": "npm run build",
    "installCommand": "npm install",
    "framework": null,
    "functions": {
        "api/index.ts": {
            "runtime": "@vercel/node"
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/index.ts"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/dist/public/assets/$1"
        },
        {
            "src": "/(.*)",
            "dest": "api/index.ts"
        }
    ]
}