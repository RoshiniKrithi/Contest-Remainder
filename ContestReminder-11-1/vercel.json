{
    "version": 2,
    "builds": [
        {
            "src": "api/index.ts",
            "use": "@vercel/node",
            "config": {
                "includeFiles": "dist/public/**"
            }
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "api/index.ts"
        },
        {
            "source": "/(.*)",
            "destination": "api/index.ts"
        }
    ]
}